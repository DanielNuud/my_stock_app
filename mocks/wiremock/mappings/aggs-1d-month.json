{
  "priority": 1,
  "request": {
    "method": "GET",
    "urlPathPattern": "/v2/aggs/ticker/([A-Za-z.]+)/range/1/day/.+/.+"
  },
  "response": {
    "status": 200,
    "headers": { "Content-Type": "application/json" },
    "transformers": ["response-template"],
    "bodyFileName": "historical/{{request.pathSegments.[3]}}_month.json"
  }
}
